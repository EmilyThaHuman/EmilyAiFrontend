{
  "compilerOptions": {
    "baseUrl": "src",
    "jsx": "react",
    "paths": {
      "app/*": ["app/*"],
      "api/*": ["api/*"],
      "assets/*": ["assets/*"],
      "styles/*": ["styles/*"],
      "components/*": ["components/*"],
      "config/*": ["config/*"],
      "contexts/*": ["contexts/*"],
      "hooks/*": ["hooks/*"],
      "layouts/*": ["layouts/*"],
      "store/*": ["store/*"],
      "types/*": ["types/*"],
      "utils/*": ["utils/*"],
      "views/*": ["views/*"],
      "*": ["*"],
      "routes/*": ["routes/*"],
      "colors/*": ["assets/themes/base/*"],
      "lib/*": ["lib/*"],
      "humanIcons/*": ["assets/humanIcons/*"]
    }
  },
  "exclude": ["node_modules", "build", "archive"],
  "include": ["src", "archive/CLEAN_UP", "archive/SAVE/heroDocs", "archive/SAVE/index.jsx"]
}
